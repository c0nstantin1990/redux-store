{
  "name": "redux-store",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c0nstantin1990/redux-store.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/c0nstantin1990/redux-store/issues"
  },
  "homepage": "https://github.com/c0nstantin1990/redux-store#readme",
  "devDependencies": {
    "concurrently": "^8.2.1",
    "redux-devtools": "^3.7.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "if-env": "^1.0.4",
    "redux": "^4.2.1",
    "stripe": "^13.6.0"
  }
}
